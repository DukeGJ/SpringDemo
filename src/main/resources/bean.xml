<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="springHelloWorld" class="main.impl.SpringHelloWorld"></bean>
    <bean id="strutsHelloWorld" class="main.impl.StructHelloWorld"></bean>
    <bean id="helloWorldService" class="main.service.HelloWorldService">
        <property name="helloWorld" ref="strutsHelloWorld"/>
    </bean>
</beans>